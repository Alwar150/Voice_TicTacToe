        <qualifiedname>ps_endpointer_t::ps_endpointer_set_timestamp_func</qualifiedname>
        <param>
          <type><ref refid="structps__endpointer__t" kindref="compound">ps_endpointer_t</ref> *</type>
          <declname>ep</declname>
        </param>
        <param>
          <type><ref refid="endpointer_8h_1ac01ff24b5365d201fb0ed51bf7e830c2" kindref="member">ps_endpointer_timestamp_cb_t</ref></type>
